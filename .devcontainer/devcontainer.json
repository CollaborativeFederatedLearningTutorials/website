{
    "build": {
		"context": "..",
		"dockerfile": "Dockerfile"
	},
    "features": {
        "ghcr.io/devcontainers/features/docker-in-docker:2": {
            "moby": false
        }
    },
    "hostRequirements": {
		"memory": "8gb",
		"storage": "32gb",
        "cpus": 4
	},
    "workspaceFolder": "/workspaces/website/tutorial",
    "postCreateCommand": "bash /workspaces/website/.devcontainer/postCreateCommand.sh",
    "postStartCommand": "bash /workspaces/website/.devcontainer/postStartCommand.sh",
    "postAttachCommand": "bash /workspaces/website/.devcontainer/postAttachCommand.sh",
    "waitFor":"postStartCommand"
  }